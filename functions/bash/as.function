function as (){
    case $# in
        0)  xsel -bo |
            while read i ;do
                aria2c -S "$i"
                echo -e "\n#################\n"
            done
            ;;
        *)  until [[ "$1" = "" ]] ;do
                aria2c -S "$1"
                echo -e "\n#################\n"
                shift
            done
            ;;
    esac
    return 0
}

