function optisvg (){
    local TT="$(mktemp)"
    case "$#" in
        0)  find "$(pwd)" -iname \*.svg |while read i ;do
                [[ ! -h "$i" ]] && echo "$i"
            done |sort >"$TT"
        ;;
        *)  until [[ "$1" = "" ]] ;do
                [[ ! -h "$i" ]] && echo "$1"
                shift
            done |sort >"$TT"
        ;;
    esac
    cat "$TT" |xargs -P9 -I{} bash -c 'svgo --multipass "{}"'
    /usr/bin/rm "$TT"
}

